<table>
    <thead>
        <tr>
            <th colspan="4">Cats TABLE</th>
        </tr>
    </thead>
    <tbody>
        <%@cats.each do |cat|%>
            <tr>
                <td> <%= cat.name%> </td>
                <td> <%= cat.age%> </td>
                <td> <%= cat.color%> </td>
                <td> <%= cat.sex%> </td>
            </tr>

        <%end%>
    </tbody>
</table>